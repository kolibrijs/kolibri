{
  "name": "@kolibrijs/parser",
  "version": "0.0.0",
  "description": "Markdown parser for Kolibri",
  "author": "nyxb <contact@nyxb.email>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nyxb",
  "homepage": "https://kolibri.show",
  "repository": {
    "type": "git",
    "url": "https://github.com/kolibrijs/kolibri"
  },
  "bugs": "https://github.com/kolibrijs/kolibri/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    },
    "./fs": {
      "types": "./fs.d.ts",
      "import": "./dist/fs.mjs",
      "require": "./dist/fs.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts src/core.ts src/fs.ts",
    "dev": "nyxr build --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@kolibrijs/types": "workspace:*",
    "js-yaml": "^4.1.0"
  }
}
