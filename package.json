{
  "name": "kolibri-monorepo",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "version": "0.0.0",
  "scripts": {
   "build": "pnpm -r --filter=./packages/** run build",
   "ci:publish": "zx scripts/publish.mjs",
   "cy": "cypress open",
   "cy:fixture": "pnpm -C cypress/fixtures/basic run dev",
   "demo:build": "zx ./scripts/demo.mjs",
   "demo:composable-vue": "pnpm -C demo/composable-vue run dev",
   "demo:dev": "pnpm -C demo/starter run dev",
   "play": "pnpm demo:dev",
   "dev": "pnpm -r --filter=./packages/** --parallel run dev",
   "lint": "eslint . --cache",
   "lint:fix": "nr lint --fix",
   "typecheck": "vue-tsc --noEmit",
   "release": "bumping package.json packages/*/package.json --all -x \"zx scripts/update-versions.mjs\"",
   "test": "vitest test"
 },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@kolibrijs/cli": "workspace:*",
    "@kolibrijs/parser": "workspace:*",
    "@kolibrijs/types": "workspace:*",
    "@nyxb/nyxi": "^0.0.31",
    "@nyxb/utils": "^0.0.3",
    "@types/cli-progress": "^3.11.5",
    "@types/codemirror": "^5.60.13",
    "@types/connect": "^3.4.38",
    "@types/debug": "^4.1.12",
    "@types/file-saver": "^2.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/katex": "^0.16.6",
    "@types/localtunnel": "^2.0.4",
    "@types/markdown-it": "^13.0.6",
    "@types/prismjs": "^1.26.3",
    "@types/prompts": "^2.4.8",
    "@types/recordrtc": "^5.6.14",
    "@types/resolve": "^1.20.5",
    "@types/semver": "^7.5.5",
    "@types/yargs": "^17.0.31",
    "@vueuse/core": "^10.6.1",
    "bumping": "^9.2.1",
    "bun": "^1.0.14",
    "bun-types": "latest",
    "conmitmoji": "^0.0.12",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "eslint": "^8.54.0",
    "esno": "^4.0.0",
    "get-component": "^0.1.3",
    "katex": "^0.16.9",
    "mermaid": "^10.6.1",
    "npm-run-all": "^4.1.5",
    "playwright-chromium": "^1.39.0",
    "styled-components": "^6.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-windicss": "^1.9.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22",
    "zx": "^7.2.3"
  }
}
